            ".parent-element": {
              "height": "auto",
              "overflow": "hidden"
            },
            ".button-container": {
              "display": "flex",
              "justify-content": "center",
              "align-items": "center"
            },
            "#show-more-btn, #show-less-btn": {
              "background-color": "white",
              "color": "black",
              "border": "2px solid black",
              "padding": "10px 20px",
              "border-radius": "999px",
              "cursor": "pointer",
              "transition": "all 0.3s ease",
              "display": "block",
              "margin-top": "2rem",
              "text-align": "center",
              "font-family": "'Poppins', sans-serif"
            },
            ".mbr-section-title": {
              "text-align": "center"
            }
